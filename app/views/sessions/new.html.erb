<h1>Login</h1>
<%= form_with(scope: :session, url: login_path,local: true) do |i|%>

    <p>
        <%= i.label :Email%><br>
        <%= i.text_field :email%>

    </p>

    <p>
        <%= i.label :Password%><br>
        <%= i.password_field :password%>
    </p>

    <%= i.submit 'Login'%>

<%end%>