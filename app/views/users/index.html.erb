<h1>Users</h1>
<table>
    <thead>
        <tr>
            <th>Username</th>
            <th>Email</th>
        </tr>
    </thead>
    <tbody>
        <% @user.each do|i| %>
        <tr>
            <td><%= i.username%></td>
            <td><%= i.email%></td>
        </tr>
        <%end%>
    </tbody>
</table>